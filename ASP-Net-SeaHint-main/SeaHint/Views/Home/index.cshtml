@{
    Layout = null;
}

@model IEnumerable<TipoPraia>

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Carrossel de Cards</title>
    <link rel="stylesheet" href="~/css/principal.css">
</head>

<body>
 @foreach (var nPraia in Model)
    {

            <h1 id="principalTitulo">Classificação Seahint</h1>



            <p id="seahint">Confira a seguir as melhores praias eleito pelo SeaHint</p>


            <h4 class="categoria">Melhores Praias para surf</h4>
            <div class="carousel-wrapper">
                <div class="carousel-container">
                    <div class="carousel" id="carousel-1">
                    <div class="carousel-item"><a class="nav-link text-dark" asp-area="" asp-controller="Praia" asp-action="PraiadeMaresias"><img src="~/img/image 9.png" alt="Card 1"><p>@nPraia.nomePraia1</p> </a></div>
                        <div class="carousel-item"><img src="~/img/image 10.png" alt="Card 2"><p class="nomeP">@nPraia.nomePraia2</p></div>
                    <div class="carousel-item"><img src="~/img/image 11.png" alt="Card 3"><p class="nomeP">@nPraia.nomePraia3</p></div>
                    <div class="carousel-item"><img src="~/img/image 12.png" alt="Card 4"><p class="nomeP">@nPraia.nomePraia4</p></div>
                    <div class="carousel-item"><img src="~/img/image 13.png" alt="Card 5"><p class="nomeP">@nPraia.nomePraia5</p></div>
                    <div class="carousel-item"><img src="~/img/image 14.png" alt="Card 6"><p class="nomeP">@nPraia.nomePraia6</p></div>
                    <div class="carousel-item"><img src="~/img/image 15.png" alt="Card 7"><p class="nomeP">@nPraia.nomePraia7</p></div>
                    <div class="carousel-item"><img src="~/img/image 16.png" alt="Card 8"><p class="nomeP">@nPraia.nomePraia8</p></div>
                    <div class="carousel-item"><img src="~/img/image 17.png" alt="Card 9"><p class="nomeP">@nPraia.nomePraia9</p></div>
                    <div class="carousel-item"><img src="~/img/image 18.png" alt="Card 10"><p class="nomeP">@nPraia.nomePraia10</p></div>
                    </div>
                    <button class="carousel-control prev" data-carousel="carousel-1" onclick="prevSlide('carousel-1')">&#9664;</button>
                    <button class="carousel-control next" data-carousel="carousel-1" onclick="nextSlide('carousel-1')">&#9654;</button>
                </div>

                <h4 class="categoria">Melhores Praias para banho</h4>

                <div class="carousel-container">
                    <div class="carousel" id="carousel-2">
                    <div class="carousel-item"><a class="nav-link text-dark" asp-area="" asp-controller="Praia" asp-action="PraiaBranca"><img src="~/img/image 19.png" alt="Card 1"> </a><p class="nomeP">@nPraia.nomePraia11</p></div>
                    <div class="carousel-item"><img src="~/img/image 20.png" alt="Card 2"><p class="nomeP">@nPraia.nomePraia12</p></div>
                    <div class="carousel-item"><img src="~/img/image 21.png" alt="Card 3"><p class="nomeP">@nPraia.nomePraia13</p></div>
                    <div class="carousel-item"><img src="~/img/image 22.png" alt="Card 4"><p class="nomeP">@nPraia.nomePraia14</p></div>
                    <div class="carousel-item"><img src="~/img/image 23.png" alt="Card 5"><p class="nomeP">@nPraia.nomePraia15</p></div>
                    <div class="carousel-item"><img src="~/img/image 24.png" alt="Card 6"><p class="nomeP">@nPraia.nomePraia16</p></div>
                    <div class="carousel-item"><img src="~/img/image 25.png" alt="Card 7"><p class="nomeP">@nPraia.nomePraia17</p></div>
                    <div class="carousel-item"><img src="~/img/image 26.png" alt="Card 8"><p class="nomeP">@nPraia.nomePraia18</p></div>
                    <div class="carousel-item"><img src="~/img/image 27.png" alt="Card 9"><p class="nomeP">@nPraia.nomePraia19</p></div>
                    <div class="carousel-item"><img src="~/img/image 28.png" alt="Card 10"><p class="nomeP">@nPraia.nomePraia20</p></div>
                    </div>
                    <button class="carousel-control prev" data-carousel="carousel-2" onclick="prevSlide('carousel-2')">&#9664;</button>
                    <button class="carousel-control next" data-carousel="carousel-2" onclick="nextSlide('carousel-2')">&#9654;</button>
                </div>




                <h4 class="categoria">Melhores Praias para caminhada</h4>
                <div class="carousel-container">
                    <div class="carousel" id="carousel-3">
                    <div class="carousel-item"><a class="nav-link text-dark" asp-area="" asp-controller="Praia" asp-action="PraiaDomingo"><img src="~/img/image 29.png" alt="Card 1"></a><p class="nomeP">@nPraia.nomePraia21</p></div>
                    <div class="carousel-item"><img src="~/img/image 30.png" alt="Card 2"><p class="nomeP">@nPraia.nomePraia22</p></div>
                    <div class="carousel-item"><img src="~/img/image 31.png" alt="Card 3"><p class="nomeP">@nPraia.nomePraia23</p></div>
                        <div class="carousel-item"><img src="~/img/image 32.png" alt="Card 4"><p class="nomeP">@nPraia.nomePraia24</p></div>
                        <div class="carousel-item"><img src="~/img/image 33.png" alt="Card 5"><p class="nomeP">@nPraia.nomePraia25</p></div>
                        <div class="carousel-item"><img src="~/img/image 34.png" alt="Card 6"><p class="nomeP">@nPraia.nomePraia26</p></div>
                        <div class="carousel-item"><img src="~/img/image 35.png" alt="Card 7"><p class="nomeP">@nPraia.nomePraia27</p></div>
                        <div class="carousel-item"><img src="~/img/image 36.png" alt="Card 8"><p class="nomeP">@nPraia.nomePraia28</p></div>
                        <div class="carousel-item"><img src="~/img/image 37.png" alt="Card 9"><p class="nomeP">@nPraia.nomePraia29</p></div>
                        <div class="carousel-item"><img src="~/img/image 38.png" alt="Card 10"><p class="nomeP">@nPraia.nomePraia30</p></div>
                    </div>
                    <button class="carousel-control prev" data-carousel="carousel-3" onclick="prevSlide('carousel-3')">&#9664;</button>
                    <button class="carousel-control next" data-carousel="carousel-3" onclick="nextSlide('carousel-3')">&#9654;</button>
                </div>
            </div>
            <script src="~/js/site.js"></script>
            <div id="footer">
                <p id="pfooter">copyright@2024 - todos os direitos atualizados</p>
            </div>
    }
</body>
</html>